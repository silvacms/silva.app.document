<?xml version="1.0" encoding="utf-8"?>
<silva xmlns="http://infrae.com/namespace/silva"
       xmlns:silva-content="http://infrae.com/namespace/metadata/silva-content"
       xmlns:silva-extra="http://infrae.com/namespace/metadata/silva-extra"
       xmlns:silva-app-document="http://infrae.com/namespace/silva-app-document"
       xmlns:silva-core-editor="http://infrae.com/namespace/silva-core-editor"
       xmlns:html="http://www.w3.org/1999/xhtml"
       silva_version="Silva 3.0a1dev">
  <publication id="folder">
    <metadata>
      <set id="silva-content">
        <silva-content:maintitle>Import folder</silva-content:maintitle>
        <silva-content:shorttitle/>
      </set>
      <set id="silva-extra">
        <silva-extra:comment/>
        <silva-extra:contactemail/>
        <silva-extra:contactname/>
        <silva-extra:content_description/>
        <silva-extra:creationtime/>
        <silva-extra:creator>manager</silva-extra:creator>
        <silva-extra:expirationtime/>
        <silva-extra:hide_from_tocs>do not hide</silva-extra:hide_from_tocs>
        <silva-extra:keywords/>
        <silva-extra:language/>
        <silva-extra:lastauthor>unknown</silva-extra:lastauthor>
        <silva-extra:modificationtime>2011-05-19T13:33:40Z</silva-extra:modificationtime>
        <silva-extra:publicationtime/>
        <silva-extra:subject/>
      </set>
      <set xmlns:silvalayout="http://infrae.com/namespace/metadata/silva-layout" id="silva-layout">
        <silvalayout:skin>Standard Issue</silvalayout:skin>
      </set>
    </metadata>
    <content>
      <default>
        <silva-app-document:document id="index">
          <workflow>
            <version id="0">
              <status>public</status>
              <publication_datetime>2011-05-19T13:33:40Z</publication_datetime>
              <expiration_datetime/>
            </version>
          </workflow>
          <content version_id="0">
            <metadata>
              <set id="silva-content">
                <silva-content:maintitle>
                  Welcome to Silva!
                </silva-content:maintitle>
                <silva-content:shorttitle/>
              </set>
              <set id="silva-extra">
                <silva-extra:comment/>
                <silva-extra:contactemail/>
                <silva-extra:contactname/>
                <silva-extra:content_description/>
                <silva-extra:creationtime
                   >2011-05-19T13:33:40Z
                </silva-extra:creationtime>
                <silva-extra:creator>manager</silva-extra:creator>
                <silva-extra:hide_from_tocs>
                  do not hide
                </silva-extra:hide_from_tocs>
                <silva-extra:keywords/>
                <silva-extra:language/>
                <silva-extra:lastauthor>manager</silva-extra:lastauthor>
                <silva-extra:modificationtime>
                  2011-05-19T13:33:40Z
                </silva-extra:modificationtime>
                <silva-extra:subject/>
              </set>
            </metadata>
            <silva-app-document:body>
              <silva-core-editor:text>
                <html:div>
                  <html:h1>Welcome to Silva!</html:h1>
                  <html:p>Welcome to Silva! This is the public view. To actually see something interesting, try adding '/edit' to your url (if you're not already editing, you can <html:a href="edit">click this link</html:a>).</html:p>
                </html:div>
              </silva-core-editor:text>
            </silva-app-document:body>
          </content>
        </silva-app-document:document>
      </default>
      <silva-app-document:document id="example">
        <workflow>
          <version id="0">
            <status>unapproved</status>
            <publication_datetime/>
            <expiration_datetime/>
          </version>
        </workflow>
        <content version_id="0">
          <metadata>
            <set id="silva-content">
              <silva-content:maintitle>Example</silva-content:maintitle>
              <silva-content:shorttitle/>
            </set>
            <set id="silva-extra">
              <silva-extra:comment/>
              <silva-extra:contactemail/>
              <silva-extra:contactname/>
              <silva-extra:content_description/>
              <silva-extra:creationtime>2011-05-19T13:33:40Z</silva-extra:creationtime>
              <silva-extra:creator>manager</silva-extra:creator>
              <silva-extra:expirationtime/>
              <silva-extra:hide_from_tocs>do not hide</silva-extra:hide_from_tocs>
              <silva-extra:keywords/>
              <silva-extra:language/>
              <silva-extra:lastauthor>unknown</silva-extra:lastauthor>
              <silva-extra:modificationtime>2011-05-19T13:33:40Z</silva-extra:modificationtime>
              <silva-extra:publicationtime/>
              <silva-extra:subject/>
            </set>
          </metadata>
          <silva-app-document:body>
            <silva-core-editor:text>
              <html:div>
                <html:h1>Example</html:h1>
                <html:p>
                  here is a paragraph...</html:p>
                <html:p>
                  then comes a <html:a class="link" reference="folder" reference-type="document link" target="_self">link</html:a>
                </html:p>
                <html:p>then comes an image :</html:p>
                <html:div class="image default">
                  <html:img alt="location.png" reference="folder/folder/image" reference-type="document image"/>
                  <html:span class="image-caption">location.png</html:span>
                </html:div>
              </html:div>
            </silva-core-editor:text>
          </silva-app-document:body>
        </content>
      </silva-app-document:document>
      <folder id="folder">
        <metadata>
          <set id="silva-content">
            <silva-content:maintitle>Folder</silva-content:maintitle>
            <silva-content:shorttitle/>
          </set>
          <set id="silva-extra">
            <silva-extra:comment/>
            <silva-extra:contactemail/>
            <silva-extra:contactname/>
            <silva-extra:content_description/>
            <silva-extra:creationtime>2011-05-19T13:33:40Z</silva-extra:creationtime>
            <silva-extra:creator>manager</silva-extra:creator>
            <silva-extra:expirationtime/>
            <silva-extra:hide_from_tocs>do not hide</silva-extra:hide_from_tocs>
            <silva-extra:keywords/>
            <silva-extra:language/>
            <silva-extra:lastauthor>unknown</silva-extra:lastauthor>
            <silva-extra:modificationtime>2011-05-19T13:33:40Z</silva-extra:modificationtime>
            <silva-extra:publicationtime/>
            <silva-extra:subject/>
          </set>
        </metadata>
        <content>
          <image id="image" web_crop="" web_format="PNG" web_scale="100%">
            <metadata>
              <set id="silva-content">
                <silva-content:maintitle>Image</silva-content:maintitle>
                <silva-content:shorttitle/>
              </set>
              <set id="silva-extra">
                <silva-extra:comment/>
                <silva-extra:contactemail/>
                <silva-extra:contactname/>
                <silva-extra:content_description/>
                <silva-extra:creationtime>2011-05-19T13:33:40Z</silva-extra:creationtime>
                <silva-extra:creator>manager</silva-extra:creator>
                <silva-extra:expirationtime/>
                <silva-extra:hide_from_tocs>do not hide</silva-extra:hide_from_tocs>
                <silva-extra:keywords/>
                <silva-extra:language/>
                <silva-extra:lastauthor>unknown</silva-extra:lastauthor>
                <silva-extra:modificationtime>2011-05-19T13:33:40Z</silva-extra:modificationtime>
                <silva-extra:publicationtime/>
                <silva-extra:subject/>
              </set>
            </metadata>
            <asset id="1" />
          </image>
        </content>
      </folder>
    </content>
  </publication>
</silva>
